{
  "name": "@shunkakinoki/renovate-config",
  "version": "1.0.3",
  "private": false,
  "homepage": "https://github.com/shunkakinoki/configurations",
  "bugs": {
    "url": "https://github.com/shunkakinoki/configurations/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shunkakinoki/configurations.git"
  },
  "license": "MIT",
  "author": "Shun Kakinoki",
  "main": "src/index.js",
  "scripts": {
    "fix": "cd ../.. && yarn run fix"
  },
  "dependencies": {
    "renovate": "^25.52.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "renovate-config": {
    "default": {
      "description": "Default config for @shunkakinoki",
      "extends": [
        "config:base"
      ],
      "automerge": true,
      "rangeStrategy": "bump",
      "prConcurrentLimit": 0,
      "prHourlyLimit": 0,
      "schedule": "every hour"
    }
  }
}
